{
  "extends": ["eslint:recommended", "plugin:node/recommended", "plugin:prettier/recommended"],
  "plugins": ["node"],
  "rules": {
      "node/no-unsupported-features": [
        "error",
        {"version": 8}
      ],
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "trailingComma": "es5"
        }
      ]
  }
}